{
    "name": "bnw/laravel-module-tools",
    "description": "Este pacote contém ferramentas para implementação e gerenciamento de módulos para o Laravel usando composer",
    "keywords": [
        "bnw",
        "development",
        "tools",
        "laravel"
    ],
    "homepage": "https://github.com/bueno-networks/laravel-module-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Pereira",
            "email": "ricardo.pereira@buenonetworks.com.br",
            "homepage": "https://www.buenonetworks.com.br",
            "role": "Tech Lead"
        },
        {
            "name": "Aristides Neto",
            "email": "aristides.neto@buenonetworks.com.br",
            "homepage": "https://www.buenonetworks.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "laravel/laravel": "^7.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Bnw\\Tools\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnw\\Tools\\ServiceProvider"
            ],
            "aliases": {
                "Tools": "Bnw\\Tools\\ToolsFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bnw\\Tools\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
